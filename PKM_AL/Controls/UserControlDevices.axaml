<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vm="using:PKM_AL"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
			 x:DataType="vm:MainWindow"
             x:Class="PKM_AL.UserControlDevices">
	<DataGrid Name="GridDevises" AutoGenerateColumns="False" SelectionMode="Single" CanUserReorderColumns="False"
			  IsReadOnly="True" GridLinesVisibility="All" ItemsSource="{Binding $self.DataContext}" >		
	</DataGrid>
</UserControl>
